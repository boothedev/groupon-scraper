### Example environment variables for local development or container runtime
# Copy this file to `.env` and edit values as needed.

# Maximum concurrent pages (int)
MAX_CONCURRENT_PAGES=4

# Run Playwright in headless mode? 1 = headless (default), 0 = headed
PLAYWRIGHT_HEADLESS=1

# Navigation timeouts and retry behaviour
PAGE_GOTO_TIMEOUT_MS=60000
PAGE_GOTO_RETRIES=2

# Logging level
LOG_LEVEL=info
